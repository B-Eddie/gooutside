{
  "manifest_version": 3,
  "name": "Go Outside - Screen Time Manager",
  "version": "1.0",
  "description": "Manage your screen time and get reminded to go outside",
  "permissions": [
    "storage",
    "alarms",
    "tabs",
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
} 